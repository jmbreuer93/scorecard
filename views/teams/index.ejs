<h1>All Teams</h1>

<% layout('layouts/boilerplate') %>

<a href="/teams/new">Add a Team</a>

<table>
    <tr>
        <th>Team Name</th>
        <th>Manager</th>
        <th>Wins</th>
        <th>Losses</th>
        <th>Winning Percentage</th>
    </tr>
    <% for (let team of teams) { %>
        <tr>
            <td><a href="/teams/<%= team._id %>"><%= team.teamName %></a></td>
            <td><%= team.manager %></td>
            <td><%= team.wins %></td>
            <td><%= team.losses %></td>  
            <td><%= team.winningPercentage %></td>
        </tr>
    <% } %>
</table>

