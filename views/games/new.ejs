<h1>New Game for <%= teamName %></h1>

<form action="">
    <label for="awayTeam">Visitor</label>
    <select name="awayTeam" id="awayTeam">
        <option value="">Select a Team</option>
        <% for(let team of teams) { %>
            <option value="<%= team._id %>"><%= team.teamName %></option>
        <% } %>
    </select>
    <label for="homeTeam">Home</label>
    <select name="homeTeam" id="homeTeam">
        <option value="">Select a Team</option>
        <% for(let team of teams) { %>
            <option value="<%= team._id %>"><%= team.teamName %></option>
        <% } %>
    </select>
    <ul>
        <% for(let player of players) { %>
            <li><%= player.firstName %></li>
        <% } %>
    </ul>
</form>

